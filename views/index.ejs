<%- include('./partials/head') %>

  <body>

    <div class="card">
      <article class="front">
        <header>
          <h1>
            <%= member.name %>
          </h1>
          <button title="uitklappen">
            <div class="icon">
              <i class="uil uil-angle-right-b"></i>
              <i class="uil uil-angle-left-b"></i>
            </div>
          </button>
        </header>
        <img src="<%= member.avatar %>" alt="Avatar van <%= member.name %>">
      </article>

      <article class="persoonlijk">
        <header>
          <h2>Wie ben ik</h2>
        </header>
        <p>
          <%= member.bio %>
        </p>

      </article>

    </div>

    <%- include('./partials/foot') %>